{
    "name": "obsidian-vault-sync",
    "version": "0.8.0",
    "description": "Obsidian Cloud Storage Sync Plugin (Google Drive)",
    "type": "module",
    "main": "dist/obsidian-vault-sync/main.js",
    "scripts": {
        "dev": "rollup -c -w",
        "build": "node update-build-hash.mjs && rollup -c",
        "build:release": "rollup -c",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "google-drive",
        "sync"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-typescript": "^12.3.0",
        "@types/diff-match-patch": "^1.0.36",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "25.2.3",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "eslint": "^9.39.2",
        "js-yaml": "^4.1.1",
        "obsidian": "latest",
        "rollup": "4.57.1",
        "rollup-plugin-copy": "^3.5.0",
        "tslib": "^2.8.1",
        "typescript": "5.9.3",
        "vitest": "^4.0.18"
    },
    "dependencies": {
        "diff-match-patch": "^1.0.5"
    }
